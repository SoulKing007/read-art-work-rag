# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here
SUPABASE_ANON_KEY=your-anon-key-here

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview
# Alternative models: gpt-3.5-turbo, gpt-4, gpt-4-turbo

# Embeddings Configuration
EMBEDDING_MODEL=text-embedding-ada-002
EMBEDDING_DIMENSION=1536
# For ada-002: dimension=1536
# For text-embedding-3-small: dimension=1536
# For text-embedding-3-large: dimension=3072

# LangChain Configuration (Optional - for tracing/debugging)
LANGCHAIN_API_KEY=
LANGCHAIN_TRACING_V2=false
LANGCHAIN_PROJECT=ready-artwork-rag

# Server Configuration
PORT=3000
NODE_ENV=development
# Options: development, production

# CORS Configuration
CORS_ORIGIN=http://localhost:8080
# For production, set to your frontend URL

# RAG Configuration
TOP_K_RETRIEVAL=10
TOP_K_CONTEXT=5
LLM_TEMPERATURE=0.3
CONFIDENCE_THRESHOLD_HIGH=0.8
CONFIDENCE_THRESHOLD_MEDIUM=0.6
