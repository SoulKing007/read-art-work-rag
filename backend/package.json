    "description": "RAG backend with LangChain and LangGraph for Ready Artwork's client knowledge system",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "nodemon --exec tsx src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
